client.config [
  '$stateProvider','$locationProvider','$urlRouterProvider'
  ($stateProvider,$locationProvider,$urlRouterProvider) ->

    # $locationProvider.html5Mode(true)
    $urlRouterProvider.otherwise('/')

    $stateProvider
      .state 'home',
        url: '/',
        templateUrl: "<%=asset_path('home.html')%>"
        controller: 'HomeCtrl'
      .state 'game',
        url: '/game'
        templateUrl: "<%=asset_path('game.html')%>"
        controllet: 'GameCtrl'

]
